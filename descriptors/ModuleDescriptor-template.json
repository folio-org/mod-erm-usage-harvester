{
  "id": "${project.artifactId}-${project.version}",
  "name": "erm-usage-harvester",
  "provides": [
    {
      "id": "erm-usage-harvester",
      "version": "1.1",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/erm-usage-harvester/start"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/erm-usage-harvester/start/{id}"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/erm-usage-harvester/impl"
        },
        {
          "methods": ["GET", "POST", "DELETE"],
          "pathPattern": "/erm-usage-harvester/settings"
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "1.0",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": ["POST", "DELETE"],
          "pathPattern": "/_/tenant"
        }
      ]
    }
  ],
  "requires": [
    {
      "id": "users-bl",
      "version": "3.3"
    },
    {
      "id": "usage-data-providers",
      "version": "2.0"
    },
    {
      "id": "aggregator-settings",
      "version": "1.0"
    },
    {
      "id": "counter-reports",
      "version": "1.0"
    }
  ],
  "launchDescriptor": {
    "dockerImage": "${project.artifactId}:${project.version}",
    "dockerArgs": {
      "HostConfig": {
        "PortBindings": {
          "8081/tcp": [
            {
              "HostPort": "%p"
            }
          ]
        }
      }
    },
    "dockerPull": false
  }
}
